for f in `ls ../data/*.blif | cut -d/ -f 3| cut -d. -f 1`; do  echo $f >> my_timing; echo "My simulator:" >> my_timing; ./3fsim ../data/${f}.blif ../data/${f}.pat ${f}.out | grep "Simulation Time" >> my_timing; echo "Slow simulator:" >> my_timing; ../data/3fsim ../data/${f}.blif ../data/${f}.pat /tmp/${f}.out | grep "Simulation Time" >> my_timing; echo "Fast simulator:" >> my_timing; ../data/3fsim_fast ../data/${f}.blif ../data/${f}.pat /tmp/${f}.out | grep "Simulation Time" >> my_timing; echo >> my_timing; done
